                    <TextBlock Name="txtUsuario" Height="22" HorizontalAlignment="Right" Margin="0,366,35,0" VerticalAlignment="Top" Width="194" />
                </Grid>
            </FixedPage>
        </PageContent>
    </FixedDocument>
</Window>
